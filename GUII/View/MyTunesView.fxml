<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="543.0" prefWidth="679.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo6.MyTunesController">
   <children>
      <Pane prefHeight="536.0" prefWidth="679.0">
         <children>
            <TableView fx:id="playlistTable" layoutX="14.0" layoutY="115.0" prefHeight="363.0" prefWidth="154.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Name" />
                <TableColumn prefWidth="75.0" text="Songs" />
              </columns>
            </TableView>
            <TableView fx:id="songTableView" layoutX="189.0" layoutY="112.0" prefHeight="363.0" prefWidth="139.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Track" />
                <TableColumn prefWidth="75.0" text="Title" />
              </columns>
            </TableView>
            <TableView fx:id="songsTable" layoutX="414.0" layoutY="112.0" prefHeight="363.0" prefWidth="229.0">
              <columns>
                <TableColumn prefWidth="75.0" text="Title" />
                <TableColumn prefWidth="75.0" text="Artist" />
                  <TableColumn prefWidth="75.0" text="Genre" />
              </columns>
            </TableView>
            <TextField layoutX="488.0" layoutY="29.0" promptText="Search..." />
            <Slider layoutX="28.0" layoutY="81.0" />
            <ProgressBar fx:id="songProgressBar" layoutX="228.0" layoutY="45.0" prefWidth="200.0" progress="0.0" />
            <Label layoutX="235.0" layoutY="68.0" prefHeight="26.0" prefWidth="188.0" text="Something is playing">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Button fx:id="previousButton" layoutX="22.0" layoutY="28.0" mnemonicParsing="false" onAction="#previousMusic" prefHeight="26.0" prefWidth="40.0" text="Back">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="playButton" layoutX="72.0" layoutY="29.0" mnemonicParsing="false" onAction="#playMusic" prefHeight="26.0" prefWidth="40.0" text="Play">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="pauseButton" layoutX="121.0" layoutY="29.0" mnemonicParsing="false" onAction="#pauseMusic" prefHeight="26.0" prefWidth="40.0" text="Pause">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="nextButton" layoutX="169.0" layoutY="29.0" mnemonicParsing="false" onAction="#nextMusic" prefHeight="25.0" prefWidth="40.0" text="Next">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
